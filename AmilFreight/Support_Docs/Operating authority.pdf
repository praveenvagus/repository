<body>
    <div class="section-default">
        <div class="div100 pull-left">
            <div class="m-signin">
                <div class="pull-left" style="width: 60%;margin-left: 20%;margin-right: 20%;">
                    <div class="content-wrap pull-left">
                        <div class="div100 pull-left position-relative">
                            <div class="col-md-5 pull-left signin-bg" style="width:36%;">
                                <img src="/assets/Styles/Images/logo_prev.png" style="padding: 45px 5px;" />
                            </div>
                            <div class="col-md-7 pull-left" style="width:64%;">
                                <div style="width: 76%;margin-left: 12%;margin-top: 9%;">
                                    <h3 class="text-center" style="font-size: 24px;color: #495462;font-weight: 600;margin-bottom: 10px;">Sign In </h3>
                                    <h6 class="text-center" style="color:#495462;">Welcome back! We are happy you like Amil Freight</h6>

                                    <form id="frmSignIn" name="frmSignIn" class="cd-form floating-labels" style="margin-top:20px;">
                                        <div class="col-md-12">
                                            <label class="cd-label" for="email">Email Address</label>
                                            <input class="form-control" id="txtusername"
                                                   type="text"
                                                   name="email"
                                                   ng-model="User.name"
                                                   maxlength="50"
                                                   ng-submit="btnSignIn"
                                                   ng-validate="req,Email Address / User Id" value="" />
                                        </div>
                                        <div class="col-md-12">
                                            <label class="cd-label" for="confirmPassword">Password</label>
                                            <input class="form-control" id="txtpassword"
                                                   type="password"
                                                   name="txtpassword"
                                                   maxlength="20"
                                                   ng-model="User.password"
                                                   ng-submit="btnSignIn"
                                                   ng-validate="req,Password" value="" />
                                        </div>
                                        <div class="clearfix col-md-12" style="margin:0px;">
                                            <ul class="cd-form-list" style="padding-left:0px;">
                                                <li>
                                                    <input type="checkbox" id="cd-checkbox-1">
                                                    <label for="cd-checkbox-1" style="color: #495462;line-height: 22px;font-weight: 600;">Remember Me</label>
                                                </li>
                                            </ul>
                                        </div>
                                        <div class="clearfix col-md-12" style="margin:0px;">
                                            <input id="btnSignIn" type="button" value="Sign In " class="div100" name="" />
                                        </div>
                                        <div class="clearfix col-md-12 text-center">
                                            <a href="/forgotpassword" class="text-center text-muted">Forgot Your Password?</a>
                                        </div>

                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                </div>
            </div>
        </div>
    </div>


